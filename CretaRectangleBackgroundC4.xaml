<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<Style x:Key="CretaRectangleBackgroundC4" TargetType="{x:Type Rectangle}">
		<Setter Property="Effect">
            <Setter.Value>
                <DropShadowEffect ShadowDepth="0" BlurRadius="20" Direction="0"/>
            </Setter.Value>
        </Setter>
        <Setter Property="Fill">
            <Setter.Value>
                <SolidColorBrush Color="{StaticResource CretaprintC4_Gris_FondoClaro}"/>
            </Setter.Value>
        </Setter>
	</Style>
</ResourceDictionary>