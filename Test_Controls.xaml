<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
    xmlns:base="clr-namespace:CretaBase"
	x:Class="CretaBase.Test_Controls"
	x:Name="UserControl"
	d:DesignWidth="1920" d:DesignHeight="1080">
	<UserControl.Resources>
		<base:InverseBooleanConverter x:Key="InverseBooleanConverter"/>
		<base:ImagePathConverter x:Key="ImagePathConverter" />
    	<!--Hides control if boolean value is true-->
        <base:BooleanToVisibilityConverter x:Key="HiddenIfTrue" ValueWhenTrue="Hidden" ValueWhenFalse="Visible"/>
        <!--Hides control if boolean value is false-->
        <base:BooleanToVisibilityConverter x:Key="HiddenIfFalse" ValueWhenTrue="Visible" ValueWhenFalse="Hidden"/>
        <!--Collapses control if boolean value is true-->
        <base:BooleanToVisibilityConverter x:Key="CollapsedIfTrue" ValueWhenTrue="Collapsed" ValueWhenFalse="Visible"/>
        <!--Collapses control if boolean value is false-->
        <base:BooleanToVisibilityConverter x:Key="CollapsedIfFalse" ValueWhenTrue="Visible" ValueWhenFalse="Collapsed"/>
	</UserControl.Resources>
	<Grid x:Name="LayoutRoot" Background="#FFFaFaFa">
		<Grid Visibility="{Binding IsFinished, Converter={StaticResource HiddenIfTrue}}"/>
		<Grid IsEnabled="{Binding NotEnabled, Converter={StaticResource InverseBooleanConverter}}"/>
		<StackPanel Orientation="Horizontal" Margin="0,0,10,0" Visibility="{Binding IsVisible, Converter={StaticResource CollapsedIfFalse}}">
			<StackPanel Orientation="Vertical" HorizontalAlignment="left" Width="auto" Margin="0,0,10,0">
				<Grid>
					<Rectangle Width="100" Height="50" Fill="{DynamicResource CretaprintC4_GrisOscuro_Seleccion_br}" />
					<TextBlock Text="#FF333333" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="LightGray"/>
				</Grid>
				<Grid>
					<Rectangle Width="100" Height="50" Fill="{DynamicResource CretaprintC4_GrisOscuro_Fondo_br}" />
					<TextBlock Text="#FF4d4d4d" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Center"/>
				</Grid>
				<Grid>
					<Rectangle Width="100" Height="50" Fill="{DynamicResource CretaprintC4_GrisClaro_Trazos_br}" />
					<TextBlock Text="#FF707070" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Center"/>
				</Grid>
				<Grid>
                	<Rectangle Width="100" Height="50" Fill="{DynamicResource CretaprintC4_GrisMedio_br}" />
					<TextBlock Text="#FFCCCCCC" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Center"/>
				</Grid>
				<Grid>
					<Rectangle Width="100" Height="50" Fill="{DynamicResource CretaprintC4_GrisClaro_Barras_br}" />
					<TextBlock Text="#FFe6e6e6" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Center"/>
				</Grid>
				<Grid>
					<Rectangle Width="100" Height="50" Fill="{DynamicResource CretaprintC4_Gris_Fondo_br}" />
					<TextBlock Text="#FFf2f2f2" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Center"/>
				</Grid>
				
				<Grid>
					<Rectangle Width="100" Height="50" Fill="{DynamicResource BotonDegradadoRojo}" />
					<TextBlock Text="BtnDegRojo" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Center"/>
				</Grid>
				<Grid>
					<Rectangle Width="100" Height="50" Fill="{DynamicResource BotonDegradadoVerde}" />
					<TextBlock Text="BtnDegVerde" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Center"/>
				</Grid>
				<Grid>
					<Rectangle Width="100" Height="50" Fill="{DynamicResource BotonDegradadoGris}" />
					<TextBlock Text="BtnDegGris" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Center"/>
				</Grid>
				<Grid>
					<Rectangle Width="100" Height="50" Fill="{DynamicResource BotonDegradadoGris2}" />
					<TextBlock Text="BtnDegGris2" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Center"/>
				</Grid>
				
				<Grid>
					<Rectangle Width="100" Height="50" Fill="{DynamicResource CretaprintC4_Verde_Light_br}" />
					<TextBlock Text="#FF73B249" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Center"/>
				</Grid>
				<Grid>
					<Rectangle Width="100" Height="50" Fill="{DynamicResource CretaprintC4_Verde_Claro_br}" />
					<TextBlock Text="#FF49b700" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Center"/>
				</Grid>
				<Grid>
					<Rectangle Width="100" Height="50" Fill="{DynamicResource CretaprintC4_Verde_br}" />
					<TextBlock Text="#FF378900" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Center"/>
				</Grid>
				<Grid>
					<Rectangle Width="100" Height="50" Fill="{DynamicResource CretaprintC4_Amarillo_br}" />
					<TextBlock Text="#FFffa600" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Center"/>
				</Grid>
				<Grid>
					<Rectangle Width="100" Height="50" Fill="{DynamicResource CretaprintC4_Naranja_br}" />
					<TextBlock Text="#FFf07500" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Center"/>
				</Grid>
				
				<Grid>
					<Rectangle Width="100" Height="50" Fill="{DynamicResource CretaprintC4_Rojo_Light_br}" />
					<TextBlock Text="#FFC64B4F" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Center"/>
				</Grid>
				<Grid>
					<Rectangle Width="100" Height="50" Fill="{DynamicResource CretaprintC4_Rojo_Claro_br}" />
					<TextBlock Text="#FFc1272d" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Center"/>
				</Grid>
				<Grid>
					<Rectangle Width="100" Height="50" Fill="{DynamicResource CretaprintC4_Rojo_br}" />
					<TextBlock Text="#FF82272d" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Center"/>
				</Grid>
				
				<Grid>
					<Rectangle Width="100" Height="50" Fill="{DynamicResource CretaprintC4_efi_azul_br}" />
					<TextBlock Text="#FF084D90" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Center"/>
				</Grid>
				<Grid>
					<Rectangle Width="100" Height="50" Fill="{DynamicResource CretaprintC4_efi_azul_light_br}" />
					<TextBlock Text="#FF7194C4" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Center"/>
				</Grid>
				<Grid>
					<Rectangle Width="100" Height="50" Fill="{DynamicResource CretaprintC4_Blanco_br}" />
					<TextBlock Text="#FFFFFFFF" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Center"/>
				</Grid>
				<Grid>
					<Rectangle Width="100" Height="50" Fill="{DynamicResource CretaprintC4_Negro_br}" />
					<TextBlock Text="#FF000000" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>
				</Grid>
			</StackPanel>
			<Separator Template="{DynamicResource CretaSeparatorVerticalC4}" Margin="10"/>
			<StackPanel Orientation="Vertical" Margin="5,10">
				<GroupBox Style="{DynamicResource CretaGroupBoxThinC4}" Width="150" Height="150" Margin="5">
					<TextBlock Text="CretaGroupBoxThinC4" VerticalAlignment="Center" FontSize="14" Height="50" TextWrapping="Wrap" HorizontalAlignment="Center"/>
				</GroupBox>
				<GroupBox Header="CretaGroupBoxC4" Template="{DynamicResource CretaGroupBoxC4}" Width="150" Height="150" Margin="5">
					<TextBlock Text="CretaGroupBoxC4" VerticalAlignment="Center" FontSize="14" Height="50" TextWrapping="Wrap" HorizontalAlignment="Center"/>
				</GroupBox>
				
				<Separator Template="{DynamicResource CretaSeparatorHorizontalC4}" Margin="0,10"/>
				
				<TextBox Text="CretaTextBoxC4" Style="{DynamicResource CretaTextBoxC4}" Height="25" Margin="10"/>
				<TextBox Text="CretaTextBoxC4" Style="{DynamicResource CretaTextBoxC4}" IsEnabled="False" Height="25" Margin="10"/>
				<TextBox Text="CretaTextBoxC4 IsReadOnly" Style="{DynamicResource CretaTextBoxC4}" IsReadOnly="True" Height="25" Margin="10"/>
				
				<Separator Template="{DynamicResource CretaSeparatorHorizontalC4}" Margin="0,10"/>
				
				<ProgressBar Value="75" IsIndeterminate="False" Height="18" Margin="10" />
				<ProgressBar IsIndeterminate="True" Height="18" Margin="10" />
				<ProgressBar Value="75" IsIndeterminate="False" Height="10" Margin="10" Style="{DynamicResource CretaProgressBarC4}" Tag="Orange" />
				<ProgressBar IsIndeterminate="True" Value="50" Height="10" Margin="10" Style="{DynamicResource CretaProgressBarC4}" Tag="Orange" />
				
				<Separator Template="{DynamicResource CretaSeparatorHorizontalC4}" Margin="0,10"/>
				<StackPanel Orientation="Horizontal">
					<Slider Orientation="Horizontal" Width="100" Maximum="100" Minimum="50" TickPlacement="Both" TickFrequency="5" HorizontalAlignment="Center" Margin="10" VerticalAlignment="Center" />
					<Slider Orientation="Vertical" Height="100" Maximum="100" Minimum="50" TickPlacement="Both" TickFrequency="5" HorizontalAlignment="Center" Margin="10"/>
				</StackPanel>
				
				<Separator Template="{DynamicResource CretaSeparatorHorizontalC4}" Margin="0,10"/>
				
				<StackPanel Orientation="Horizontal">
					<CheckBox IsChecked="True" Content="CheckBox" HorizontalAlignment="Center" Template="{DynamicResource CretaCheckBoxC4}" Margin="5,10,5,5"/>
					<CheckBox IsChecked="False" Content="Opt2" HorizontalAlignment="Center" Template="{DynamicResource CretaCheckBoxC4}" Margin="5,20,5,10"/>
				</StackPanel>
				
				<Separator Template="{DynamicResource CretaSeparatorHorizontalC4}" Margin="0,10"/>
				
				<StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
					<CheckBox IsChecked="True" HorizontalAlignment="Center" Margin="5" Template="{DynamicResource CretaCheckBoxFavoriteC4}"/>
					<CheckBox IsChecked="False" HorizontalAlignment="Center" Margin="5" Template="{DynamicResource CretaCheckBoxFavoriteC4}"/>
				</StackPanel>
				
				<Separator Template="{DynamicResource CretaSeparatorHorizontalC4}" Margin="0,10"/>
				
				<StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
					<RadioButton Content="Opt.1" GroupName="test" Margin="5" Template="{DynamicResource CretaRadioButtonControlC4}" IsChecked="True"/>
					<RadioButton Content="Opt.2" GroupName="test" Margin="5" Template="{DynamicResource CretaRadioButtonControlC4}"/>
				</StackPanel>
				<StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
					<RadioButton Content="Opt A" GroupName="test1" Margin="5" Template="{DynamicResource CretaRadioButtonControlSmC4}" IsChecked="True"/>
					<RadioButton Content="Opt B" GroupName="test1" Margin="5" Template="{DynamicResource CretaRadioButtonControlSmC4}"/>
				</StackPanel>
				
			</StackPanel>
			<Separator Template="{DynamicResource CretaSeparatorVerticalC4}" Margin="10"/>
			<StackPanel Orientation="Vertical" Margin="5,10">
				<Button Margin="10,5" Width="120" Height="120" Style="{DynamicResource CretaButtonRoundC4}" Tag="Botón normal" 
						IsEnabled="True" base:CretaProperties.IsButtonSelected="False" >
                	<ContentControl Style="{DynamicResource Icon_Restart}" Foreground="Orange" base:CretaProperties.Scale="0.7"/>
                    <base:CretaProperties.Content2>
                    	<ContentControl Style="{DynamicResource Icon_Restart}" Foreground="White" base:CretaProperties.Scale="0.7"/>
					</base:CretaProperties.Content2>
                </Button>
				<Button Margin="10,5" Width="120" Height="120" Style="{DynamicResource CretaButtonRoundC4}" Tag="Botón marcado como IsButtonSelected" IsEnabled="True" base:CretaProperties.IsButtonSelected="True">
                	<ContentControl Style="{DynamicResource Icon_Restart}" Foreground="Orange" base:CretaProperties.Scale="0.7"/>
                    <base:CretaProperties.Content2>
                    	<ContentControl Style="{DynamicResource Icon_Restart}" Foreground="White" base:CretaProperties.Scale="0.7"/>
					</base:CretaProperties.Content2>
                </Button>
				<Button Margin="10,5" Width="120" Height="120" Style="{DynamicResource CretaButtonRoundC4}" Tag="Botón deshabilitado" IsEnabled="False" base:CretaProperties.IsButtonSelected="False">
                	<ContentControl Style="{DynamicResource Icon_Restart}" Foreground="White" base:CretaProperties.Scale="0.7"/>
                    <base:CretaProperties.Content2>
                    	<ContentControl Style="{DynamicResource Icon_Restart}" Foreground="Orange" base:CretaProperties.Scale="0.7"/>
					</base:CretaProperties.Content2>
                </Button>
				
				<Separator Template="{DynamicResource CretaSeparatorHorizontalThinC4}" Margin="0,10"/>
								
				<Button Margin="0,5" Height="50" Style="{DynamicResource CretaButtonTxtBlockC4}" Tag="Orange">
						<TextBlock Text="BotónTextblock" Foreground="White"/>
				</Button>
                <Button Margin="0,5" Height="50" Style="{DynamicResource CretaButtonTxtBlockC4}" Tag="Orange" IsEnabled="False">
                    <TextBlock Text="BotónTextblock" Foreground="White"/>
                </Button>
				<Button Margin="0,5" Height="50" Style="{DynamicResource CretaButtonTxtBlockC4}" Tag="Orange" base:CretaProperties.IsButtonSelected="True">
                    <TextBlock Text="BotónTextblock" Foreground="White"/>
                </Button>
				
				<Separator Template="{DynamicResource CretaSeparatorHorizontalThinC4}" Margin="0,10"/>
				
				<Button Margin="10,5" Content="BotónLight" Width="80" Height="80" Style="{DynamicResource CretaButtonRoundLightC4}" />
				<Button Margin="10,5" Content="BotónLight" Width="80" Height="80" Style="{DynamicResource CretaButtonRoundLightC4}" IsEnabled="False"/>
				
				<Separator Template="{DynamicResource CretaSeparatorHorizontalThinC4}" Margin="0,10"/>
				
				<Button Margin="0,5" Height="50" Style="{DynamicResource CretaButtonTabC4}" >
						<TextBlock Text="BotónTab" Foreground="White"/>
				</Button>
				<Button Margin="0,5" Height="50" Style="{DynamicResource CretaButtonTabC4}" Tag="Orange">
						<TextBlock Text="BotónTab" Foreground="Black"/>
				</Button>
                <Button Margin="0,5" Height="50" Style="{DynamicResource CretaButtonTabC4}" Tag="Orange" IsEnabled="False">
                    <TextBlock Text="BotónTab" Foreground="White"/>
                </Button>
				<Button Margin="0,5" Height="50" Style="{DynamicResource CretaButtonTabC4}" base:CretaProperties.IsButtonSelected="True">
                    <TextBlock Text="BotónTab" Foreground="White"/>
                </Button>
				
            </StackPanel>
			<Separator Template="{DynamicResource CretaSeparatorVerticalThinC4}" Margin="10"/>
			<StackPanel Orientation="Vertical" Margin="5,10">
				<Button Margin="10" Width="75" Height="100" Style="{DynamicResource CretaButtonSquareC4}" Tag="Botón normal" IsEnabled="True" base:CretaProperties.IsButtonSelected="False">
                	<ContentControl Style="{DynamicResource Icon_Restart}" Foreground="White" base:CretaProperties.Scale="0.7"/>
                    <base:CretaProperties.Content2>
                    	<ContentControl Style="{DynamicResource Icon_Restart}" Foreground="Orange" base:CretaProperties.Scale="0.7"/>
					</base:CretaProperties.Content2>
                </Button>
				<Button Margin="10,0,10,10" Width="75" Height="125" Style="{DynamicResource CretaButtonSquareC4}" Tag="Botón marcado como IsButtonSelected" IsEnabled="True" base:CretaProperties.IsButtonSelected="True">
                	<ContentControl Style="{DynamicResource Icon_Restart}" Foreground="White" base:CretaProperties.Scale="0.7"/>
                    <base:CretaProperties.Content2>
                    	<ContentControl Style="{DynamicResource Icon_Restart}" Foreground="Orange" base:CretaProperties.Scale="0.7"/>
					</base:CretaProperties.Content2>
                </Button>
								
				<Button Margin="10,0,10,10" Width="75" Height="120" Style="{DynamicResource CretaButtonSquareC4}" Tag="Botón deshabilitado" IsEnabled="False" base:CretaProperties.IsButtonSelected="False">
                	<ContentControl Style="{DynamicResource Icon_Restart}" Foreground="White" base:CretaProperties.Scale="0.7"/>
                    <base:CretaProperties.Content2>
                    	<ContentControl Style="{DynamicResource Icon_Restart}" Foreground="Orange" base:CretaProperties.Scale="0.7"/>
					</base:CretaProperties.Content2>
                </Button>
				
				<Separator Template="{DynamicResource CretaSeparatorHorizontalC4}" Margin="0,10"/>
				
				<Button Margin="10" Width="75" Height="75" Style="{DynamicResource CretaButtonSquareLightC4}" Tag="Normal" IsEnabled="True" base:CretaProperties.IsButtonSelected="False">
                	<ContentControl Style="{DynamicResource Icon_Restart}" Foreground="White" base:CretaProperties.Scale="0.7"/>
                    <base:CretaProperties.Content2>
                    	<ContentControl Style="{DynamicResource Icon_Restart}" Foreground="Orange" base:CretaProperties.Scale="0.7"/>
					</base:CretaProperties.Content2>
                </Button>								
				<Button Margin="10" Width="75" Height="75" Style="{DynamicResource CretaButtonSquareLightC4}" Tag="Disabled" IsEnabled="False" base:CretaProperties.IsButtonSelected="False">
                	<ContentControl Style="{DynamicResource Icon_Restart}" Foreground="White" base:CretaProperties.Scale="0.7"/>
                    <base:CretaProperties.Content2>
                    	<ContentControl Style="{DynamicResource Icon_Restart}" Foreground="Orange" base:CretaProperties.Scale="0.7"/>
					</base:CretaProperties.Content2>
                </Button>
				
				<Separator Template="{DynamicResource CretaSeparatorHorizontalC4}" Margin="0,10"/>
			
				<TextBlock Text="TextBlock CretaTextBlockC4" Style="{DynamicResource CretaTextBlockC4}" Background="White" Width="120" TextWrapping="Wrap" TextAlignment="Center"/>
				
				<Separator Template="{DynamicResource CretaSeparatorHorizontalC4}" Margin="0,10"/>
								
				<Button Margin="0,10" Style="{DynamicResource CretaButtonContentC4}" >
					<Grid Background="#FFeeeeee">
                    	<ContentControl Style="{DynamicResource Icon_Bullet}" Width="24" Height="24" Foreground="{DynamicResource CretaprintC4_Amarillo_br}"/>
						<TextBlock Text="CretaButtonContentC4" VerticalAlignment="Center" Foreground="Black"/>
					</Grid>
                </Button>
				
				<Button Margin="0,10" Style="{DynamicResource CretaButtonContentC4}" IsEnabled="False">
					<Grid Background="#FFeeeeee">
                    	<ContentControl Style="{DynamicResource Icon_Bullet}" Width="24" Height="24" Foreground="{DynamicResource CretaprintC4_Amarillo_br}"/>
						<TextBlock Text="CretaButtonContentC4" VerticalAlignment="Center" Foreground="Black"/>
					</Grid>
                </Button>
				
				<Button Margin="0,10" Style="{DynamicResource CretaButtonContentC4}" base:CretaProperties.IsButtonSelected="True">
					<Grid Background="#FFeeeeee">
                    	<ContentControl Style="{DynamicResource Icon_Bullet}" Width="24" Height="24" Foreground="{DynamicResource CretaprintC4_Amarillo_br}"/>
						<TextBlock Text="CretaButtonContentC4" VerticalAlignment="Center" Foreground="Black"/>
					</Grid>
                </Button>
				
				<Separator Template="{DynamicResource CretaSeparatorHorizontalC4}" Margin="0,10"/>
				
				<Button Margin="0,10" Style="{DynamicResource CretaButtonContentSelectedC4}" >
					<Grid Background="#FFeeeeee" d:IsLocked="True">
                    	<ContentControl Style="{DynamicResource Icon_Bullet}" Width="24" Height="24" Foreground="{DynamicResource BotonDegradadoGris2}"/>
						<TextBlock Text="CretaButtonContentSelectedC4" VerticalAlignment="Center" FontSize="8" Foreground="Black"/>
					</Grid>
                </Button>
				
				<Button Margin="0,10" Style="{DynamicResource CretaButtonContentSelectedC4}" IsEnabled="False">
					<Grid Background="#FFeeeeee">
                    	<ContentControl Style="{DynamicResource Icon_Bullet}" Width="24" Height="24" Foreground="{DynamicResource BotonDegradadoGris2}"/>
						<TextBlock Text="CretaButtonContentSelectedC4" VerticalAlignment="Center" FontSize="8" Foreground="Black"/>
					</Grid>
                </Button>
				
				<Button Margin="0,10" Style="{DynamicResource CretaButtonContentSelectedC4}" Tag="Selected">
					<Grid Background="#FFeeeeee">
                    	<ContentControl Style="{DynamicResource Icon_Bullet}" Width="24" Height="24" Foreground="{DynamicResource BotonDegradadoGris2}"/>
						<TextBlock Text="CretaButtonContentSelectedC4" VerticalAlignment="Center" FontSize="8" Foreground="Black"/>
					</Grid>
                </Button>
				
				
			</StackPanel>
			<Separator Template="{DynamicResource CretaSeparatorVerticalC4}" Margin="10"/>
			<StackPanel Orientation="Vertical" Margin="5,10">
				<ContentControl Style="{DynamicResource LogoCreta}" Margin="0,20"/>
				
				<ToggleButton IsChecked="True" Template="{DynamicResource CretaToggleButtonC4}" base:CretaProperties.Scale="0.75" HorizontalAlignment="Center" Margin="5"/>
				<ToggleButton IsChecked="false" Template="{DynamicResource CretaToggleButtonC4}" base:CretaProperties.Scale="0.75" HorizontalAlignment="Center" Margin="5"/>
				<ToggleButton IsChecked="True" Template="{DynamicResource CretaToggleButtonC4}" base:CretaProperties.Scale="1.0" HorizontalAlignment="Center" Margin="5"/>
				
				<Separator Template="{DynamicResource CretaSeparatorHorizontalThinC4}" Margin="0,10"/>

				<ContentControl Style="{DynamicResource Icon_Screen}" Foreground="Gray"/>
								
				<base:CretaTextBoxNumericC4 />
				
				<base:CretaTextBoxNumericC4 IsEnabled="False"/>
				
				<base:CretaTextBoxNumericC4 Value="3" IsReadOnly="True"/>
				
				<base:CretaTextBoxNumericC4 SmallSize="True" Width="140"/>
				
				<StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
					<base:CretaTextBoxNumericC4 Vertical="True" Margin="5,0" />
					<base:CretaTextBoxNumericC4 Vertical="True" SmallSize="True" Margin="5,0"/>
				</StackPanel>
				
				<Separator Template="{DynamicResource CretaSeparatorHorizontalThinC4}" Margin="0,10"/>
				
				<ContentControl Style="{DynamicResource Icon_Screen}" Foreground="Gray"/>
				
				<base:CretaTextBoxArrowsUpDownC4 />
				
				<base:CretaTextBoxArrowsUpDownC4 IsEnabled="False"/>
				
				<base:CretaTextBoxArrowsUpDownC4 Value="3" IsReadOnly="True"/>
				
				<base:CretaTextBoxArrowsUpDownC4 SmallSize="True" Width="140"/>
				
				<StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
					<base:CretaTextBoxArrowsUpDownC4 Vertical="True" Margin="5,0" />
					<base:CretaTextBoxArrowsUpDownC4 Vertical="True" SmallSize="True" Margin="5,0"/>
				</StackPanel>
				
			</StackPanel>
			<Separator Template="{DynamicResource CretaSeparatorVerticalC4}" Margin="10"/>
			<UniformGrid Margin="10" Columns="13">
				<ContentControl Style="{DynamicResource Icon_home}" />
				<ContentControl Style="{DynamicResource Icon_console}" />
				<ContentControl Style="{DynamicResource Icon_user}" />
	            <ContentControl Style="{DynamicResource Check_Ok}" Foreground="{DynamicResource CretaprintC4_Verde_br}" base:CretaProperties.Scale="4"/>
	            <ContentControl Style="{DynamicResource Check_Bad}" Foreground="{DynamicResource CretaprintC4_Rojo_Claro_br}" base:CretaProperties.Scale="4"/>
		
				<ContentControl Style="{DynamicResource Icon_Bullet}" Width="24" Height="24" Foreground="{DynamicResource BotonDegradadoVerde}"/>
				<ContentControl Style="{DynamicResource Icon_Plus}" Width="24" Height="24" Foreground="{DynamicResource CretaprintC4_Verde_br}"/>
				<ContentControl Style="{DynamicResource Icon_Minus}" Width="24" Height="24" Foreground="{DynamicResource CretaprintC4_Verde_br}"/>
				<ContentControl Style="{DynamicResource Icon_TEST}" />
				<ContentControl Style="{DynamicResource Icon_RightArrow}" />
				<ContentControl Style="{DynamicResource Icon_Start}"/>
				<ContentControl Style="{DynamicResource Icon_Stop}" />
				<ContentControl Style="{DynamicResource Icon_Pause}" />
	            <ContentControl Style="{DynamicResource Icon_File}" />
